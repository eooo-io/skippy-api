{
    "name": "eooo/skippy-api",
    "description": "SkippyAPI with Open Swoole",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "vlucas/phpdotenv": "^5.6",
        "illuminate/database": "^11.36",
        "symfony/console": "^7.2"
    }
,
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^7.2",
        "phpunit/phpunit": "^11",
        "mockery/mockery": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.65",
        "pestphp/pest": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "fix": "php-cs-fixer fix --config=.php-cs-fixer.dist.php"
    }
}
