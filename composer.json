{
    "name": "eooo/skippy-api",
    "description": "SkippyAPI with Open Swoole",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "vlucas/phpdotenv": "^5.6",
        "illuminate/database": "^11.36"
    }
,
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^7.2",
        "pestphp/pest": "^3.7",
        "phpunit/phpunit": "^11.5",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
